# Local Keycloak Configuration for Frontend
# Copy this to .env to use local Docker Keycloak

# Local Keycloak URL (Docker instance)
VITE_KEYCLOAK_BASE_URL=http://localhost:8085/

# Keycloak Configuration
VITE_KEYCLOAK_REALM=kymatic
VITE_KEYCLOAK_CLIENT_ID=kymatic-react-client

# Backend Services
VITE_TENANT_SERVICE_URL=http://localhost:8083
VITE_API_SERVICE_URL=http://localhost:8084
VITE_GATEWAY_SERVICE_URL=http://localhost:8081

